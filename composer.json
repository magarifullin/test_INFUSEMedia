{
    "name": "test/infusemedia",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "psr/log": "^3.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        }
    },
    "config": {
        "platform": {
            "php": "8.0"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
